@using System.Diagnostics
@{
    Process p = ViewBag.Data as Process;
    //come as array of objects. but want to come as array of processes. Cast means force it.

}

<h1>
    @ViewBag.Title
</h1>

<table border="1">

    
        <tr>
            <td>
                @p.Id
            </td>
           <td>
               @p.ProcessName
           </td>
            <td>@p.HandleCount</td>
        </tr>
    
</table>